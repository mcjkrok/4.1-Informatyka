from math import sqrt
pary = ""
with open("pary.txt") as f:
    pary = f.readlines()

liczby = []
znaki = []

for linia in pary:
    linia.strip()
    liczba, znak = linia.split()
    liczby.append(int(liczba))
    znaki.append(znak)
liczby_pierwsze = []
for liczba in range(3, 100):
    if all(liczba % i != 0 for i in range(2, int(sqrt(liczba)) + 1)):
       liczby_pierwsze.append(liczba)

def czy_pierwsza(liczba):
    return True if liczba in liczby_pierwsze else False

for liczba in liczby:
    if liczba % 2 == 0:
        for i in range(3, liczba//2 + 1):
            if czy_pierwsza(i) and czy_pierwsza(liczba-i):
                print(liczba, i, liczba-i)
                break
